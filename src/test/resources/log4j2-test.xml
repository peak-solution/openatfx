<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="WARN" shutdownHook="disable" monitorInterval="5">
	
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%-5p %c{1} - %m%n" />
		</Console>
	</appenders>
	<loggers>
		<logger name="com.peaksolution.openatfx" level="info" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<root level="warn">
			<appender-ref ref="Console" />
		</root>
	</loggers>

</configuration>