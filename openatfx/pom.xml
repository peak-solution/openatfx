<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.rechner</groupId>
	<artifactId>openatfx</artifactId>
	<name>OpenATFX</name>
	<version>0.0.1-SNAPSHOT</version>
	<description>Open source ASAM ODS ATFX driver</description>
	<url>http://openatfx.sourceforge.net</url>

	<developers>
		<developer>
			<name>Christian Rechner</name>
		</developer>
	</developers>

	<scm>
		<url>https://openatfx.svn.sourceforge.net/svnroot/openatfx/</url>
		<connection>scm:svn:https://openatfx.svn.sourceforge.net/svnroot/openatfx/trunk</connection>
		<developerConnection>scm:svn:https://openatfx.svn.sourceforge.net/svnroot/openatfx/trunk</developerConnection>
	</scm>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.asam</groupId>
			<artifactId>ods</artifactId>
			<version>5.2.0</version>
		</dependency>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.1.1</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.2</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.9</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<modules>
	</modules>

</project>